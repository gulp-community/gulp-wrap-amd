{
  "name": "gulp-wrap-amd",
  "version": "0.3.0",
  "description": "Wrap files with an AMD wrapper",
  "keywords": [
    "gulpplugin",
    "amd",
    "wrap"
  ],
  "repository": "https://github.com/phated/gulp-wrap-amd.git",
  "author": "Blaine Bublitz <blaine@iceddev.com>",
  "dependencies": {
    "gulp-util": "~2.2",
    "lodash": "~2.4.1",
    "through2": "~0.4.1"
  },
  "main": "index.js",
  "engines": {
    "node": ">= 0.10"
  },
  "scripts": {
